<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>Panther.jar</jar>
  <outfile>panther.exe</outfile>
  <errTitle>Panther Error</errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>graphics\panther-logo.ico</icon>
  <jre>
    <path></path>
    <minVersion>1.5.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
  </jre>
  <versionInfo>
    <fileVersion>0.9.0.0</fileVersion>
    <txtFileVersion>0.9</txtFileVersion>
    <fileDescription>Panther</fileDescription>
    <copyright>Copyright 2012, Sankha Narayan Guria</copyright>
    <productVersion>0.9.0.0</productVersion>
    <txtProductVersion>0.9</txtProductVersion>
    <productName>Panther</productName>
    <companyName></companyName>
    <internalName>Panther</internalName>
    <originalFilename>panther.exe</originalFilename>
  </versionInfo>
  <messages>
    <startupErr>An error occurred while starting Panther.</startupErr>
    <bundledJreErr>Panther was configured to use a bundled Java Runtime Environment but the runtime is missing or corrupted.</bundledJreErr>
    <jreVersionErr>Panther requires a Java Runtime Environment</jreVersionErr>
    <launcherErr>The registry refers to a nonexistent Java Runtime Environment installation or the runtime is corrupted.</launcherErr>
    <instanceAlreadyExistsMsg>A Panther instance is already running.</instanceAlreadyExistsMsg>
  </messages>
</launch4jConfig>